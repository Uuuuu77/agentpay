{
  "version": 2,
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "functions": {
    "app/api/watcher/start/route.ts": {
      "maxDuration": 300
    },
    "app/api/delivery/start/route.ts": {
      "maxDuration": 300
    },
    "app/api/services/generate/route.ts": {
      "maxDuration": 300
    },
    "app/api/webhooks/payment-confirmed/route.ts": {
      "maxDuration": 300
    },
    "app/api/admin/stats/route.ts": {
      "maxDuration": 60
    }
  }
}
